    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Platforms>AnyCPU;x86;x64</Platforms>
    <Version>1.0.0</Version>
    <Authors>Josué Clément</Authors>
    <Company />
    <Copyright>Copyright © 2021 Josué Clément</Copyright>
    <PackageProjectUrl>https://github.com/mod6991/Byte.Toolkit.Crypto</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mod6991/Byte.Toolkit.Crypto.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
